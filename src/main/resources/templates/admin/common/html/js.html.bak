<!-- JavaScript -->
<div  th:fragment="onload_js">	
	<script type="text/javascript">
	window.rootPath="[[${rootPath}]]";
	</script>
	<script type="text/javascript" th:src="@{{path}static/admin/jquery/2.2.3/jquery.js(path=${rootPath})}"></script>
   
